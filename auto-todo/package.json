{
  "activationEvents": [],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "auto-todo.helloWorld",
        "title": "Auto Todo: Hello World"
      },
      {
        "command": "auto-todo.runAutoTodo",
        "title": "Auto Todo: Run Auto Todo"
      },
      {
        "command": "auto-todo.debug",
        "title": "Auto Todo: Debug Mode On/Off"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "auto-todo.runAutoTodo",
          "when": "view == auto-todo-view"
        },
        {
          "command": "auto-todo.debug",
          "when": "view == auto-todo-view"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "contents": "View and manage your automated TODO lists here.",
          "contextualTitle": "Auto Todo",
          "id": "auto-todo-view",
          "name": "Auto Todo"
        }
      ]
    },
    "viewsWelcome": [
      {
        "contents": "[Run Auto Todo](command:auto-todo.runAutoTodo)",
        "view": "auto-todo-view"
      }
    ]
  },
  "description": "The package for use to automate creation and updating TODO lists from comments collected from the project's files.",
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "displayName": "auto_todo",
  "engines": {
    "vscode": "^1.104.0"
  },
  "main": "./out/extension.js",
  "name": "auto-todo",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src",
    "pretest": "npm run compile && npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.0.1"
}
